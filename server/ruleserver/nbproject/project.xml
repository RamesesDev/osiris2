<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.j2ee.ejbjarproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2ee-ejbjarproject/3">
            <name>ruleserver</name>
            <minimum-ant-version>1.6</minimum-ant-version>
            <included-library files="1">file.reference.antlr-runtime-3.1.3.jar</included-library>
            <included-library files="1">file.reference.drools-api-5.1.1.jar</included-library>
            <included-library files="1">file.reference.drools-compiler-5.1.1.jar</included-library>
            <included-library files="1">file.reference.drools-core-5.1.1.jar</included-library>
            <included-library files="1">file.reference.drools-decisiontables-5.1.1.jar</included-library>
            <included-library files="1">file.reference.janino-2.5.15.jar</included-library>
            <included-library files="1">file.reference.joda-time-1.6.jar</included-library>
            <included-library files="1">file.reference.mvel2-2.0.16.jar</included-library>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>rameses-common</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
    </configuration>
</project>
